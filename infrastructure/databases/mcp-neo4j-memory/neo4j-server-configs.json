{
  "mcpServers": {
    "neo4j-memory": {
      "command": "uvx",
      "args": [
        "mcp-neo4j-memory@0.4.1"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "Scaibu@123",
        "NEO4J_DATABASE": "neo4j",
        "NEO4J_TRANSPORT": "stdio",
        "NEO4J_NAMESPACE": "memory-kg"
      }
    },
    "neo4j-cypher": {
      "command": "uvx",
      "args": [
        "mcp-neo4j-cypher@0.4.1"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "Scaibu@123",
        "NEO4J_DATABASE": "neo4j",
        "NEO4J_TRANSPORT": "stdio",
        "NEO4J_NAMESPACE": "cypher-kg",
        "NEO4J_READ_TIMEOUT": "60",
        "NEO4J_RESPONSE_TOKEN_LIMIT": "4000",
        "NEO4J_READ_ONLY": "false"
      }
    },
    "neo4j-data-modeling": {
      "command": "uvx",
      "args": [
        "mcp-neo4j-data-modeling@0.5.0"
      ],
      "env": {
        "NEO4J_TRANSPORT": "stdio",
        "NEO4J_NAMESPACE": "data-modeling"
      }
    }
  }
}
